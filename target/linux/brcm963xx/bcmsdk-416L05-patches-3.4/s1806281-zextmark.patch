+#define ZLD_IPTABLES_UINDEX_INTERNAL_DEFAULT		-1
+/*******************************************************************
+*	ZLD Feature Internal
+*******************************************************************/
+
+/*******************************************************************
+*	ZLD Feature Name Definitions
+*******************************************************************/
+
+#endif /* _ZLD_INTERNAL_SPEC_H */
